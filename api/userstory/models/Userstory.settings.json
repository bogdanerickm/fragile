{
  "connection": "default",
  "collectionName": "userstories",
  "info": {
    "name": "userstory",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "title": {
      "required": true,
      "type": "string"
    },
    "description": {
      "required": true,
      "type": "text"
    },
    "developer": {
      "model": "user",
      "plugin": "users-permissions"
    },
    "acceptancecriterias": {
      "collection": "acceptancecriteria",
      "via": "userstory"
    },
    "story_points": {
      "enum": [
        "1",
        "2",
        "3",
        "5",
        "8",
        "13"
      ],
      "type": "enumeration"
    },
    "sprint": {
      "model": "sprint",
      "via": "userstories"
    }
  }
}